{
  "name": "teamdaily-client",
  "version": "3.0.0",
  "description": "TeamDaily client",
  "main": "src",
  "lint-staged": {
    "gitDir": "..",
    "linters": {
      "*.js": [
        "./node_modules/.bin/prettier --single-quote --print-width 100 --trailing-comma all --write",
        "git add"
      ],
      "*.json": [
        "./node_modules/.bin/prettier --parser json --print-width 100 --write",
        "git add"
      ],
      "*.css": [
        "./node_modules/.bin/prettier --parser postcss --print-width 100 --write",
        "git add"
      ],
      "*.pcss": [
        "./node_modules/.bin/prettier --parser postcss --print-width 100 --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf ./dist",
    "lint": "eslint src --ext js",
    "babel-node": "babel-node",
    "build:client": "cross-env NODE_ENV=production webpack --progress --display-optimization-bailout",
    "build": "yarn run clean && yarn run build:client",
    "start": "cross-env NODE_ENV=development webpack-dev-server --port 8765 --history-api-fallback --hot --inline --progress",
    "test": "jest",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fraktio/teamdaily"
  },
  "keywords": [
    "TeamDaily"
  ],
  "author": "Fraktio",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "firebase": "^4.6.1",
    "firebaseui": "^2.4.1",
    "immutable": "^3.8.2",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-addons-perf": "^15.4.1",
    "react-dom": "^16.2.0",
    "react-fa": "^5.0.0",
    "react-firebaseui": "^1.0.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-intl": "^2.4.0",
    "react-keydown": "^1.9.6",
    "react-masonry-component": "^6.0.2",
    "react-modal": "^2.2.2",
    "react-modal-dialog": "^4.0.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "recompose": "^0.26.0",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-promise-middleware": "5.0.0",
    "redux-thunk": "^2.1.0",
    "screenfull": "^3.3.2",
    "styled-components": "^3.1.6",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@dr-kobros/broilerplate": "^0.13.4",
    "@dr-kobros/broilerplate-postcss": "^0.13.6",
    "@firebase/app-types": "^0.1.0",
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "dotenv": "^5.0.0",
    "empty": "^0.10.1",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.6.0",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.2.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.1.0",
    "normalize.css": "^7.0.0",
    "postcss": "^6.0.16",
    "postcss-advanced-variables": "^2.3.0",
    "postcss-color-function": "^4.0.1",
    "postcss-loader": "^2.1.1",
    "postcss-nested": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "postcss-partial-import": "^4.1.0",
    "postcss-preset-env": "^2.1.0",
    "prettier": "^1.10.2",
    "react-inspector": "^2.2.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "3"
  }
}
